<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <!--tools:context="org.astri.spitfire.NewActivity"-->
    <!--tools:showIn="@layout/activity_new">-->

    <!--app:layout_behavior="@string/appbar_scrolling_view_behavior"-->
    <!--
    android:max="":设置最大进度
    android:progress=""：设置当前进度
    android:secondaryProgress=""：设置第二进度
    android:indeterminate=""：设置是否为不明确进度进度条
    android:thumb="":设置seekbar滑动快的图片
    android:progressDrawable="":进度条背景图片
    android:thumbTint="":滑动块颜色,API21以上才有效
    -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:background="#21D347">

        <Button
            android:id="@+id/Cancel_bt"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="start"
            android:paddingBottom="5dip"
            android:paddingTop="20dip"
            android:textAllCaps="false"
            android:text="Cancel"
            android:textColor="#FFFFFF"
            android:textSize="20dp" />

        <TextView
            android:id="@+id/NewActivity_tv"
            android:layout_width="203dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="New Activity"
            android:textColor="#FFFFFF"
            android:textSize="24sp"
            android:visibility="visible"
            tools:text="New Activity" />

        <Button
            android:id="@+id/Save_bt"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="start"
            android:paddingBottom="5dip"
            android:paddingTop="20dip"
            android:textAllCaps="false"
            android:text="Save"
            android:textColor="#FFFFFF"
            android:textSize="20dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="10dp"
            android:background="#9FB6CD" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/Name_tv"
            android:layout_width="105dp"
            android:layout_height="40dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:gravity="start"
            android:text="  Name"
            android:padding="5dip"
            android:textColor="#000000"
            android:textSize="20sp"
            android:visibility="visible"
            tools:text="   Name" />

        <EditText
            android:id="@+id/Name_ev"
            android:layout_width="283dp"
            android:layout_height="40dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@null"
            android:padding="5dip"
            android:gravity="start"
            android:textColor="#000000"
            android:textSize="20sp"

            android:visibility="visible" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="10dp"
            android:background="#9FB6CD" />
    </LinearLayout>
    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >

        <EditText
            android:id="@+id/Start_tv"
            android:layout_width="326dp"
            android:layout_height="42dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:gravity="start"
            android:text="Start"
            android:padding="5dip"
            android:textColor="#000000"
            android:textSize="20sp"
            android:visibility="visible"
            tools:text="    Start" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#9FB6CD" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <EditText
            android:id="@+id/End_tv"
            android:layout_width="384dp"
            android:layout_height="42dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:gravity="start"
            android:text="  End"
            android:padding="5dip"
            android:textColor="#000000"
            android:textSize="20sp"
            android:visibility="visible"
            tools:text="   End" />

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#9FB6CD" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/seekBar"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:textSize="20sp"
            android:textColor="#000000"
            android:padding="5dip"
            android:layout_margin="5dp"
            android:layout_marginBottom="23dp"
            android:text="  Warmup: 15 mins" />

        <SeekBar
            android:id="@+id/seekBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="5dp"
            android:indeterminate="false"
            android:padding="5dip"
            android:max="60"
            android:progress="15"
            android:secondaryProgress="0"
            android:thumbTint="#0099FF" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="#9FB6CD" />
        </LinearLayout>
        <TextView
            android:id="@+id/result1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/seekBar1"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_margin="5dp"
            android:padding="5dip"
            android:textSize="20sp"
            android:textColor="#000000"
            android:layout_marginBottom="23dp"
            android:text="  Cooldown: 15 mins" />

        <SeekBar
            android:id="@+id/seekBar1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/result"
            android:layout_marginTop="5dp"
            android:indeterminate="false"
            android:padding="5dip"
            android:max="60"
            android:progress="15"
            android:secondaryProgress="0"
            android:thumbTint="#0099FF" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="10dp"
            android:background="#9FB6CD" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <TextView
            android:id="@+id/PulseVoltage_tv"
            android:layout_width="187dp"
            android:layout_height="37dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:gravity="start"
            android:text="  Pulse Voltage/mV"
            android:textColor="#000000"
            android:padding="5dip"
            android:textSize="20sp"
            android:visibility="visible"
            tools:text="   Pulse Voltage/mV" />

        <EditText
            android:id="@+id/PulseVoltage_ev"
            android:layout_width="57dp"
            android:layout_height="37dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@null"
            android:gravity="start"
            android:textColor="#000000"
            android:padding="5dip"
            android:textSize="20sp"
            android:visibility="visible" />

        <TextView
            android:id="@+id/PulseVoltagemv_tv"
            android:layout_width="105dp"
            android:layout_height="37dp"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:gravity="start"
            android:text="  mV"
            android:textColor="#000000"
            android:padding="5dip"
            android:textSize="20sp"
            android:visibility="visible"
            tools:text="   mV" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#9FB6CD" />
    </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >

            <TextView
                android:id="@+id/PulseWidth_tv"
                android:layout_width="271dp"
                android:layout_height="37dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:gravity="start"
                android:text="  Pulse Width/ms"
                android:textColor="#000000"
                android:padding="5dip"
                android:textSize="20sp"
                android:visibility="visible"
                tools:text="   Pulse Width/ms" />

            <EditText
                android:id="@+id/PulseWidth_ev"
                android:layout_width="142dp"
                android:layout_height="38dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@null"
                android:gravity="start"
                android:textColor="#000000"
                android:padding="5dip"
                android:textSize="20sp"
                android:visibility="visible" />

            <TextView
                android:id="@+id/PulseWidthms_tv"
                android:layout_width="192dp"
                android:layout_height="38dp"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:gravity="start"
                android:text="  ms"
                android:textColor="#000000"
                android:padding="5dip"
                android:textSize="20sp"
                android:visibility="visible"
                tools:text="   ms" />
        </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#9FB6CD" />
    </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >

                <TextView
                    android:id="@+id/Pulseinterval_tv"
                    android:layout_width="186dp"
                    android:layout_height="38dp"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:text="  Pulse interval/ms"
                    android:textColor="#000000"
                    android:padding="5dip"
                    android:textSize="20sp"
                    android:visibility="visible"
                    tools:text="   Pulse interval/ms" />

                <EditText
                    android:id="@+id/Pulseinterval_ev"
                    android:layout_width="56dp"
                    android:layout_height="38dp"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="start"
                    android:textColor="#000000"
                    android:padding="5dip"
                    android:textSize="24sp"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/Pulseintervalms_tv"
                    android:layout_width="107dp"
                    android:layout_height="38dp"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:text="  ms"
                    android:textColor="#000000"
                    android:padding="5dip"
                    android:textSize="24sp"
                    android:visibility="visible"
                    tools:text="   ms" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="55dp"
             />
    </LinearLayout>
